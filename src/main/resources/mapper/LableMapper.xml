<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.winterchen.dao.LableDao" >
  <sql id="BASE_TABLE">
    lable
  </sql>

  <sql id="BASE_COLUMN">
    lable_id,lable_name,update_time
  </sql>

  <resultMap id="BaseResultMap" type="com.winterchen.model.Lable">
    <result property="lableId" column="lable_id" />
    <result property="lableName" column="lable_name" />
    <result property="updateTime" column="update_time" />
  </resultMap>


  <select id="getLables" resultType="com.winterchen.model.Lable" resultMap="BaseResultMap">
      SELECT
        <include refid="BASE_COLUMN"/>
      FROM
        <include refid="BASE_TABLE"/>
  </select>


</mapper>